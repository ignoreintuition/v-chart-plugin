    "config/*",
    "dist/*",
    "docs/*",
    "test/*"
  ],
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "test": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
